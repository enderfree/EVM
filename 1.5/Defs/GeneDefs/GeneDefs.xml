<Defs>
	<GeneDef>
		<defName>EVM_SnakeMaw</defName>
		<label>Snake Maw</label>
		<description>Pawns with this gene can swallow creatures up to twice their size.</description>
		<iconPath>UI/Icons/Genes/EVM_SnakeMaw</iconPath>
		<iconColor>(255,255,255)</iconColor>
		<biostatCpx>1</biostatCpx>
		<biostatMet>1</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>maw</symbol>
				</li>
				<li>
					<symbol>stretch</symbol>
				</li>
				<li>
					<symbol>vor</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="EVM.GeneExtension">
				<setMawSize>2</setMawSize>
			</li>
		</modExtensions>
	</GeneDef>
	
