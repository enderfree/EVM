<Defs>
	<AbilityDef ParentName="AbilityTouchBase">
		<defName>EVM_ConsumeFatal</defName>
		<label>Consume (Fatal)</label>
		<description>Go put food where it belongs.</description>
		<iconPath>UI/Icons/Genes/EVM_FatalStomach</iconPath>
		<category>Ability</category>
		<displayOrder>600</displayOrder>
		<aiCanUse>true</aiCanUse>
		<hostile>true</hostile>
		<targetRequired>true</targetRequired>
		<displayGizmoWhileUndrafted>true</displayGizmoWhileUndrafted>
		<casterMustBeCapableOfViolence>false</casterMustBeCapableOfViolence>
		<verbProperties>
			<verbClass>Verb_CastAbilityTouch</verbClass>
			<drawAimPie>false</drawAimPie>
			<range>-1</range>
			<warmupTime>3</warmupTime>
			<targetParams>
				<canTargetSelf>false</canTargetSelf>
				<canTargetBuildings>false</canTargetBuildings>
				<canTargetMechs>true</canTargetMechs>
				<canTargetBloodfeeders>true</canTargetBloodfeeders>
			</targetParams>
		</verbProperties>
		<comps>
			<li Class="EVM.Abilities.ConsumeFatal_AbilityEffect"/>
		</comps>
	</AbilityDef>
	
