		<!-- Healing -->
		
	<GeneDef>
		<defName>EVM_DigestionWorker_Tend</defName>
		<label>Tending Stomach</label>
		<description>Adds a stomach that tends to wounds rather than digesting the preys.</description>
		<iconPath>UI/Icons/Genes/EVM_TendingStomach</iconPath>
		<displayCategory>EVM_DigestionWorker</displayCategory>
		<displayOrderInCategory>10</displayOrderInCategory>
		<iconColor>(255,255,255)</iconColor>
		<abilities>
			<li>EVM_ConsumeTend</li>
		</abilities>
		<descriptionHyperlinks>
			<AbilityDef>EVM_ConsumeTend</AbilityDef>
		</descriptionHyperlinks>
		<biostatCpx>3</biostatCpx>
		<biostatMet>2</biostatMet>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>gastro</symbol>
				</li>
				<li>
					<symbol>tending</symbol>
				</li>
				<li>
					<symbol>safe</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="EVM.GeneExtension">
				<figurativeTracks>
					<li>
						<defName>EVM_XenoDefaultTendingTract</defName>
						<purpose>Heal</purpose>
						<track>
							<li>
								<figurativeStomach>
									<actualPart>Stomach</actualPart>
									<baseDamage>10</baseDamage>
									<digestionEfficiancy>1</digestionEfficiancy>
									<digestionDamageType>AcidBurn</digestionDamageType>
									<comfort>1</comfort>
									<!--<canDigest></canDigest>-->
									<deadline>180000</deadline>
									<digestionWorker>EVM.Digestion.DigestionWorker_Tend</digestionWorker>
									<grantsNutrition>false</grantsNutrition>
									<nutritionCost>0.1</nutritionCost>
									<customReleaseWorker>EVM.Release.CustomReleaseWorker_NoBedRestNeed</customReleaseWorker>
								</figurativeStomach>
							</li>
						</track>
					</li>
				</figurativeTracks>
			</li>
		</modExtensions>
	</GeneDef>
	
